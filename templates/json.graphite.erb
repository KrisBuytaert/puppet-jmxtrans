{
"servers": [ {
    "host": "<%= jmxHost %>",
    "port": "<%= jmxPort %>",
    "queries": [ { 
        "obj": "<%= objectType %>",
        "attr": [ <%= attributes %> ],
        <% if resultAlias != '' -%>"resultAlias": "<%= resultAlias %>",<% end %>
        "outputWriters": [ { 
            "@class": "<%= outputWriterClass %>",
            "settings": { 
                "host": "<%= graphiteHost %>",
                "port": <%= graphitePort %>,
                <% if graphiteTypeNames != "" -%>"typeNames": [ "<%= graphiteTypeNames %>" ],<% end %>
            }
        }]
    }],
    "queryIntervalInSeconds": <%= queryIntervalInSeconds %>,
    "numQueryThreads": <%= numQueryThreads %>,
    "exportIntervalInSeconds": <%= exportIntervalInSeconds %>,
    "numExportThreads": <%= numExportThreads %>,
    "exportBatchSize": <%= exportBatchSize %>,
}]
}
