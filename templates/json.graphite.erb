{
"servers": [ {
    "host": "<%= jmxHost %>",
    "port": "<%= jmxPort %>",
    <% if serverAlias != '' -%>"alias": "<%= serverAlias %>",<% end %>
    "queries": [ { 
        "obj": "<%= objectType %>",
        "attr": [ <%= attributes %> ],
        <% if resultAlias != '' -%>"resultAlias": "<%= resultAlias %>",<% end %>
        "outputWriters": [ { 
            "@class": "<%= outputWriterClass %>",
            "settings": { 
                "host": "<%= graphiteHost %>",
                "port": <%= graphitePort %><% if graphiteTypeNames != "" -%>,<% end %>
                <% if graphiteTypeNames != '' -%>"typeNames": [ <%= graphiteTypeNames %> ]<% end %><% if graphiteRootPrefix != '' -%>,<% end %>
                <% if graphiteRootPrefix != '' -%>"rootPrefix": "<%= graphiteRootPrefix %>"<% end %>
            }
        }]
    }],
    "numQueryThreads": <%= numQueryThreads %><% if cronExpression != "" -%>,<% end %>
    <% if cronExpression != '' -%>"cronExpression": "<%= cronExpression %>"<% end %>
}]
}
