{
"servers": [ {
    "host": "<%= jmxHost %>",
    "port": "<%= jmxPort %>",
    "queries": [ { 
        "obj": "<%= objectType %>",
        "attr": [ <%= attributes %> ],
        <% if resultAlias != '' -%>"resultAlias": "<%= resultAlias %>",<% end %>
        "outputWriters": [ { 
            "@class": "<%= outputWriterClass %>",
            "settings": { 
                "host": "<%= graphiteHost %>",
                "port": <%= graphitePort %><% if graphiteTypeNames != "" -%>,
                "typeNames": [ "<%= graphiteTypeNames %>" ]<% end %>
            }
        }]
    }],
    "numQueryThreads": <%= numQueryThreads %><% if cronExpression != "" -%>,
    "cronExpression": "<%= cronExpression %>"<% end %>
}]
}
